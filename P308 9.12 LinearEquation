
class LinearEquation {
	int a,b,c,d,e,f;
	int x,y;
	LinearEquation(int a,int b,int c,int d,int e,int f){
		this.a=a;
		this.b=b;
		this.c=c;
		this.d=d;
		this.e=e;
		this.f=f;
	}
	int getA(){
		return a;
	}
	int getB(){
		return b;
	}
	int getC(){
		return c;
	}
	int getD(){
		return d;
	}
	int getE(){
		return e;
	}
	int getF(){
		return f;
	}
	int getX(){
        return x=(e*d-b*f)/(a*d-b*c);
	}
	int getY(){
		return y=(a*f-e*c)/(a*d-b*c);
	}
	boolean isSolvabe(){
		if(a*d-b*c==0){
			return false;
		}
		else{
			return true;
		}
	}

}
